	}
	function tech_link(name) {
		if (!(name in tech_links))
			throw new Error('No URL for: ' + name)
		return tech_links[name]
	}

mixin lk(name)
	a.lk(href='#{tech_link(name)}')= name

mixin lilk(name)
	li
		+lk(name)

mixin inline(...names)
	ul.inline
		each name in names
			li= name

mixin used(...names)
	ul.used
		each name in names
			+lilk(name)

mixin links(...names)
	ul.inline
		each name in names
			+lilk(name)

mixin icon(iconName)
	i(class='fa fa-#{iconName}')

mixin sec(name, iconName)
	section
		h1 #[+icon(iconName)] #{name}
		block
