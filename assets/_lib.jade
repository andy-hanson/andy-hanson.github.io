	function techLink(name) {
		if (!(name in techLinks))
			throw new Error(`No URL for: ${name}`)
		return techLinks[name]
	}

mixin lk(name)
	if name instanceof Array
		- var relatedNames = name.slice(1).map(_ => `<a href='${techLink(_)}'>${_}</a>`).join(', ')
		span #[a.lk(href='#{techLink(name[0])}')= name[0]] (!{relatedNames})
	else
		a.lk(href='#{techLink(name)}')= name

mixin lilk(name)
	li
		+lk(name)

mixin inline(...names)
	ul.inline
		each name in names
			li= name

mixin used(...names)
	ul.used
		each name in names
			+lilk(name)

mixin links(...names)
	ul.inline
		each name in names
			+lilk(name)

mixin icon(iconName)
	i(class='fa fa-#{iconName}')

mixin sec(name, iconName)
	section
		h1 #[+icon(iconName)] #{name}
		block
